+alias org.eolang.collections.multimap
+alias org.eolang.hamcrest.assert-that
+alias org.eolang.txt.regex
+home https://github.com/objectionary/eo
+package org.eolang.txt
+junit
+version 0.0.0

[] > r_match_compare_trivial
  assert-that > @
    TRUE
    $.equal-to TRUE

[] > r_match_regex_with_options
  QQ.io.stdout > @
    QQ.txt.sprintf
      "HEY, Regex_with_options = %s\n"
      r.
        compile.
          regex "/([a-z]+)/id"

[] > r_match_regex_txt
  QQ.io.stdout > @
    QQ.txt.sprintf
      "HEY, Regex_txt = %s\n"
      regex_txt.
        match.
          compile.
            regex "/([a-z]+[a]*)/i"
          "hello"

[] > r_match_nfa_by_sym_check
  assert-that > @
    max_state.
      nfa_by_sym.
        match.
          compile.
            regex "/([a-z]+[a]*)/i"
          "hello"
        "b"
    $.equal-to 1

[] > r_match_nfa_by_sym_check1
  QQ.io.stdout > @
    QQ.txt.sprintf "Hey, max_state = %d\n"
      max_state.
        nfa_by_sym.
          match.
            compile.
              regex "/([a-z]+[a]*)/i"
            "hello"
          "b"

[] > print_each
  [first second] > pair
    plus. > as-hash
      second.as-hash
      first.as-hash.times 100
  (multimap *).with > mmp
    pair (memory 7) "A"
    memory 1
  mmp.keys.each > @
    [key_i]
      QQ.io.stdout > @
        QQ.txt.sprintf
          "Hello, i = %d\n"
          key_i.first

[] > r_match_list_by_len_elem
  assert-that > @
    (regex.match.list_by_len_elem 7 6).length
    $.equal-to 7

[] > r_match_increase_transition
  regex.match.transition > tr0!
    "b"
    memory 0
    memory 1

  tr0.increase_by 5 > tr1
  seq > @
    QQ.io.stdout
      QQ.txt.sprintf
        "Hello, src1 = %d, dest1 = %d\n    src1 = %d, dest0 = %d\n"
        tr1.src
        tr1.dest
        tr0.src
        tr0.dest
    assert-that
      tr1.src
      $.equal-to 5
